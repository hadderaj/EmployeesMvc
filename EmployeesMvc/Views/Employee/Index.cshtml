@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model EmployeesMvc.Models.Employee[]

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>View</title>
</head>
<body>
    <h1>List of Employees</h1>
    <div>
        <ul>
            @foreach (var item in Model)
            {
                <li>Name:@item.Name E-Mail:@item.Email</li>

            }


        </ul>

        <a asp-controller="Employee" asp-action="Create">Create new employee</a>

    </div>
</body>
</html>
